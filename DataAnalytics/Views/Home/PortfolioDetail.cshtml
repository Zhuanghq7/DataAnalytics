
@{
    ViewBag.Title = "PortfolioDetail";
}

<!DOCTYPE html>
<html>

<head>
    <title>Data Analytics - User Portfolio</title>

    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!-- bootstrap -->
    <link href="~/Content/bootstrap/bootstrap.css" rel="stylesheet" />

    <link href="~/Content/bootstrap/bootstrap-responsive.css" rel="stylesheet" />
    <link href="~/Content/bootstrap/bootstrap-overrides.css" type="text/css" rel="stylesheet" />


    <!-- libraries -->
    <link href="~/Content/lib/font-awesome.css" type="text/css" rel="stylesheet" />

    <!-- global styles -->
    <link rel="stylesheet" type="text/css" href="~/Content/layout.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/elements.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/icons.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/jquery.dataTables.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/jquery.dataTables.min.css" />
    <!-- this page specific styles -->
    <link rel="stylesheet" href="~/Content/compiled/user-profile.css" type="text/css" media="screen" />

    <link rel="stylesheet" href="http://apps.bdimg.com/libs/jqueryui/1.10.4/css/jquery-ui.min.css">
    <script src="http://apps.bdimg.com/libs/jquery/1.10.2/jquery.min.js"></script>
    <script src="http://apps.bdimg.com/libs/jqueryui/1.10.4/jquery-ui.min.js"></script>

    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
</head>

<body>

    <!-- navbar -->
    <div class="navbar navbar-inverse">
        <div class="navbar-inner">
            <button type="button" class="btn btn-navbar visible-phone" id="menu-toggler">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>

            <a class="brand" href="index.html">Data Analytics</a>

            <ul class="nav pull-right">
                <li class="hidden-phone">
                    <input class="search" type="text" />
                </li>


                <li class="dropdown">
                    <a href="signin.html" class="dropdown-toggle hidden-phone" data-toggle="dropdown">
                        Account...
                        <b class="caret"></b>
                    </a>
                    <ul class="dropdown-menu">
                        <li><a href="signin.html">log in</a></li>
                        <li><a href="signin.html">Log out</a></li>
                    </ul>
                </li>
                <li class="settings hidden-phone">
                    <a href="signin.html" role="button">
                        Log out
                    </a>
                </li>

            </ul>
        </div>
    </div>
    <!-- end navbar -->
    <!-- sidebar -->
    <div id="sidebar-nav">
        <ul id="dashboard-menu">
            <li>
                <a href="index.html">
                    <i class="icon-home"></i>
                    <span>Summary</span>
                </a>
            </li>
            <li>
                <a href="chart.html">
                    <i class="icon-signal"></i>
                    <span>Charts</span>
                </a>
            </li>
            <li class="active">
                <div class="pointer">
                    <div class="arrow"></div>
                    <div class="arrow_border"></div>
                </div>
                <a class="dropdown-toggle" href="#">
                    <i class="icon-group"></i>
                    <span>Users</span>
                    <i class="icon-chevron-down"></i>
                </a>
                <ul class="submenu">
                    <li><a href="portfolio.html">User portfolio</a></li>
                </ul>
            </li>

        </ul>
    </div>
    <!-- end sidebar -->
    <!-- main container -->
    <div class="content">

        <!-- settings changer -->
        <div class="skins-nav">
            <a href="#" class="skin first_nav selected">
                <span class="icon"></span><span class="text">Default</span>
            </a>
            <a href="#" class="skin second_nav" data-file="css/skins/dark.css">
                <span class="icon"></span><span class="text">Dark skin</span>
            </a>
        </div>

        <div class="container-fluid">
            <div id="pad-wrapper" class="user-profile">
                <!-- header -->
                <div class="row-fluid header">
                    <div class="span8">
                        <img src="~/Content/img/contact-profile.png" class="avatar img-circle" />
                        <h3 class="name">Alejandra Galván Castillo</h3>
                        <span class="area">Graphic Designer</span>
                    </div>
                </div>

                <div class="row-fluid profile">
                    <!-- bio, new note & orders column -->
                    <div class="span9 bio">
                        <div class="profile-box">


                            <h6>User Portfolio</h6>
                            <br />



                            <div class="table_row">
                                <div class="search_bar" style="display:block">
                                    <input type="text" class="form-control" placeholder="symbol" id="input_find" />
                                </div>
                                <span class="input-group-btn">
                                    <button class="btn btn-search" id="add_btn" style="margin-left: 3px">add symbol</button>
                                    <button class="btn btn-search save_portfolio_btn" id="save_portfolio" style="margin-left: 3px">save portfolio</button>
                                </span>
                            </div>





                            <!-- recent orders table -->
                            <table border="1" class="display" id="portfolioTable">
                                <thead>
                                    <tr>
                                        <th>Open</th>
                                        <th>Close</th>
                                        <th>High</th>
                                        <th>Low</th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>


                </div>
            </div>
        </div>
    </div>
    <!-- end main container -->


    <script type="text/javascript" src="~/Scripts/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.dataTables.min.js"></script>

    <!-- this page specific scripts -->
    <script type="text/javascript" src="~/Scripts/portfoliodetail.js"></script>
    <script src="~/Scripts/theme.js"></script>

</body>

</html>

